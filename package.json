{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" CHOKIDAR_USEPOLLING=false WATCHPACK_POLLING=false next build",
    "vercel-build": "prisma generate && next build",
    "build:safe": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" CHOKIDAR_USEPOLLING=false WATCHPACK_POLLING=false TAILWIND_DISABLE_TOUCH=true next build",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "check-format": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "check": "npm run lint && npm run check-format && npm run test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.13.0",
    "@react-google-maps/api": "^2.19.3",
    "@supabase/supabase-js": "^2.53.0",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.537.0",
    "next": "15.3.8",
    "next-auth": "^4.24.11",
    "prisma": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-image-crop": "^11.0.10",
    "recharts": "^3.0.2",
    "swr": "^2.3.4",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}
